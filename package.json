{
    "name": "flwinvoices",
    "version": "1.0.0",
    "description": "Flutterwave OSS Invoices",
    "productName": "Invoices",
    "cordovaId": "com.flutterwave.invoices",
    "keywords": [
        "laravel",
        "php",
        "vuejs",
        "vue",
        "quasar",
        "js",
        "phone",
        "tablet",
        "desktop",
        "spa",
        "pwa",
        "website",
        "electron"
    ],
    "author": {
        "name": "Arinze Salvation",
        "email": "salvation@flutterwavego.com",
        "url": "https://www.flutterwave.com"
    },
    "license": "MIT",
    "homepage": "https://quasar-framework.org",
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "lint": "eslint --ext .js,.vue src",
        "q-ssr-build": "quasar build -m ssr",
        "q-ssr-dev": "quasar dev -m ssr",
        "q-spa-build": "quasar build",
        "q-spa-dev": "quasar dev -H app.flwinvoices.test --port 8080"
    },
    "devDependencies": {
        "@quasar/app": "^1.9.6",
        "@quasar/quasar-app-extension-dotenv": "^1.0.5",
        "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.8.0",
        "eslint-loader": "^3.0.4",
        "eslint-plugin-vue": "^6.2.2",
        "laravel-mix": "^5.0.4",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "webpack-require-http": "^0.4.3"
    },
    "dependencies": {
        "@babel/helper-compilation-targets": "^7.10.4",
        "@quasar/extras": "^1.9.5",
        "axios": "^0.19.2",
        "chart.js": "^2.9.3",
        "collect.js": "^4.28.2",
        "lodash": "^4.17.15",
        "moment": "^2.25.3",
        "moment-timezone": "^0.5.29",
        "quasar": "^1.13.2",
        "vee-validate": "^2.2.15",
        "vue": "^2.6.11",
        "vue-chartjs": "^3.5.0",
        "vue-i18n": "^8.17.6",
        "vue-json-excel": "^0.2.98",
        "vue-moment": "^4.1.0",
        "vue-recaptcha": "^1.3.0",
        "vue-router": "^3.1.6",
        "vue-template-compiler": "2.6.12",
        "vue2-animate": "^2.1.4",
        "vuex": "^3.4.0",
        "vuex-map-fields": "^1.4.0"
    },
    "engines": {
        "node": ">= 8.9.0",
        "npm": ">= 5.6.0",
        "yarn": ">= 1.6.0"
    },
    "browserslist": [
        "last 1 version, not dead, ie >= 11"
    ]
}
